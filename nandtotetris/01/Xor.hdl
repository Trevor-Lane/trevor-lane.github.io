/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=f);
    Nand(a=a, b=f, out=s);
    Nand(a=f, b=b, out=t);
    Nand(a=s, b=t, out=out);
}